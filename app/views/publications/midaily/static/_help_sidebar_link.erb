<li class="<%= ( action_name == path || (action_name == "new" && path == "/contacts") ) ? 'current' : '' %>">
	<% case label %>
	<% when "Back to Search", "Contact Us", label.first == "#" %>
		<%= link_to label, path, :id => path %>
	<% else %>
		<% ">>>>>>>>>> PATH IS: #{path}" %>
		<%= link_to label,  path %>
	<% end %>
</li>
